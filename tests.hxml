

############ TEST XMPP PACKET

-cp src
-cp ../core/src//hx
-neko bin/tests/xmpp/test_packet.n
-main test.xmpp.TestPacket

--next
-cp src
-cp ../core/src//hx
-swf9 bin/tests/xmpp/test_packet.swf
-swf-header 1200:800:60:ffffff
-main test.xmpp.TestPacket

--next
-cp src
-cp ../core/src//hx
-swf-version 8
-swf-header 1200:800:60:ffffff
-swf bin/tests/xmpp/test_packet_8.swf
-main test.xmpp.TestPacket

--next
-cp src
-cp ../core/src//hx
-js bin/tests/xmpp/test_packet.js
-main test.xmpp.TestPacket

--next
-cp src
-cp ../core/src//hx
-php bin/tests/xmpp
--php-front test_packet.php
-main test.xmpp.TestPacket




