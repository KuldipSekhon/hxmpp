{
  "name": "HXMPP example",
  "version": "0.4.13",
  "manifest_version": 2,
  "minimum_chrome_version": "31",
  "description": "HXMPP example application",
  "app": {
    "background": {
      "scripts": ["background.js"] }
  },
  "permissions": [
    "notifications",
    "storage",
    "http://*/*", "https://*/*",
    {
      "socket": [
        "tcp-connect:*:5222",
        "tcp-connect:*:5223"
      ]
    }
  ]
}
